    <iconPath>BS_RaceIcons/Robot</iconPath>
    <combatPowerFactor>1</combatPowerFactor>
    <inheritable>true</inheritable>
    <genes>
      <li>Body_Standard</li>
      <li>Beard_NoBeardOnly</li>
      <li>Hair_BaldOnly</li>
      <li>Hair_InkBlack</li>
      <li>Skin_DeepRed</li>
      <li MayRequire="RedMattis.BigSmall.Core">BS_SkinBloodRed</li>
      <li MayRequire="RedMattis.BigSmall.Core">BS_Skin_DarkAsh</li>

      <!-- <li>ArchiteMetabolism</li> -->
      <li>Deathrest</li>
      <li>Hemogenic</li>
      <!-- <li>HemogenDrain</li> -->
      <li>Bloodfeeder</li>
      <li>DarkVision</li>
      <li>Aggression_HyperAggressive</li>
      <li>UVSensitivity_Mild</li>
      <li>Ageless</li>
      
      <li MayRequire="RedMattis.BigSmall.Core">BS_EltexPsychic</li>
      <li MayRequire="RedMattis.BigSmall.Core">BS_NoFood_Hemogenic</li>
      <li MayRequire="RedMattis.BigSmall.Core">VU_GreaterHemogenDrain</li>
      <li MayRequire="RedMattis.BigSmall.Core">BS_Deathlike</li>
      <li MayRequire="RedMattis.BigSmall.Core">BS_PsychicWarrior</li>
      <li MayRequire="RedMattis.BigSmall.Core">VU_Darkness_MoveSpeed_Quick</li>

      <li MayRequire="vanillaracesexpanded.sanguophage">VRE_SanguoFeeder</li>
      <!-- <li MayRequire="vanillaracesexpanded.sanguophage">VRE_Hemosmosis</li> -->

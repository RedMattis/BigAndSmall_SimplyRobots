    <modExtensions>
      <li Class="BetterPrerequisites.XenotypeExtension">
        <setRace>BS_CommandoBot</setRace>
        <forceRace>true</forceRace>
      </li>
    </modExtensions>
  </XenotypeDef>

  

</Defs>
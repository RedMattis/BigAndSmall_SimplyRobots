      <li>Fur</li>
    </exclusionTags>
    <bodyType>Standard</bodyType>
    <modExtensions>
      <li Class="BigAndSmall.PawnExtension">
        <bodyPaths>
          <Body_Female>Pawns/Commando/Naked_Female</Body_Female>
          <Body_Male>Pawns/Commando/Naked_Male</Body_Male>
        </bodyPaths>
        <bodyMaterial>
          <shader>CutoutComplex</shader>
